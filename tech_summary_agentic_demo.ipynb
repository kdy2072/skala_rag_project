    "class TechSummaryAgent:\n",
    "    def __init__(self):\n",
    "        self.tech_validator = TechExistenceValidatorTool()\n",
    "        self.core_analyzer = CoreTechAnalyzerTool()\n",
    "        self.ip_validator = IP_ResearchValidatorTool()\n",
    "        self.competitive_agent = CompetitiveLandscapeAgent()\n",
    "        self.llm = ChatOpenAI(model=\"gpt-4o-mini\", temperature=0)\n",
    "        \n",
    "        # 실행 로그\n",
    "        self.execution_log = []\n",
    "        \n",
    "    def run(self, input_json: dict) -> dict:\n",
    "        print(\"🚀 Agentic TechSummaryAgent 시작\")\n",
    "        print(f\"📥 분석 대상: {input_json.get('company_name', '')}\")\n",
    "        \n",
    "        # 상태 초기화\n",
    "        state = TechAnalysisState(\n",
    "            company_name=input_json.get(\"company_name\", \"\"),\n",
    "            core_tech=input_json.get(\"core_tech\", \"\"),\n",
    "            patents=input_json.get(\"patents\", \"\"),\n",
    "            investments=input_json.get(\"investments\", \"\"),\n",
    "            pros=input_json.get(\"pros\", \"\"),\n",
    "            owner=input_json.get(\"owner\", \"\"),\n",
    "            tech_exists=False\n",
    "        )\n",
    "        \n",
    "        try:\n",
    "            # Agentic 워크플로우 실행\n",
    "            self.execution_log = []\n",
    "            \n",
    "            # 1단계: 기술 실존성 검증\n",
    "            self.execution_log.append(\"기술 실존성 검증 시작\")\n",
    "            state = self.tech_validator.run(state)\n",
    "            self.execution_log.append(f\"검증 결과: {state.get('tech_exists', False)}\")\n",
    "            \n",
    "            # 조건부 분기\n",
    "            if state.get(\"tech_exists\", False):\n",
    "                self.execution_log.append(\"상세 분석 경로 선택\")\n",
    "                \n",
    "                # 2단계: 핵심 기술 분석\n",
    "                self.execution_log.append(\"핵심 기술 분석 시작\")\n",
    "                state = self.core_analyzer.run(state)\n",
    "                \n",
    "                # 3단계: IP 검증\n",
    "                self.execution_log.append(\"IP 검증 시작\")\n",
    "                state = self.ip_validator.run(state)\n",
    "                \n",
    "                # 4단계: 경쟁 분석\n",
    "                self.execution_log.append(\"경쟁 분석 시작\")\n",
    "                state = self.competitive_agent.run(state)\n",
    "                \n",
    "                # 5단계: 최종 요약\n",
    "                self.execution_log.append(\"최종 요약 생성\")\n",
    "                state = self._generate_final_summary(state)\n",
    "            else:\n",
    "                self.execution_log.append(\"기본 분석 경로 선택\")\n",
    "                state = self._generate_basic_summary(state)\n",
    "            \n",
    "            # 결과 병합\n",
    "            result = input_json.copy()\n",
    "            result.update({\n",
    "                \"tech_summary\": state.get(\"tech_summary\", \"\"),\n",
    "                \"strengths_and_weaknesses\": state.get(\"strengths_and_weaknesses\", \"\"),\n",
    "                \"differentiation_points\": state.get(\"differentiation_points\", \"\"),\n",
    "                \"technical_risks\": state.get(\"technical_risks\", \"\"),\n",
    "                \"patents_and_papers\": state.get(\"patents_and_papers\", [])\n",
    "            })\n",
    "            \n",
    "            self.execution_log.append(\"워크플로우 완료\")\n",
    "            print(\"🎉 Agentic 워크플로우 완료\")\n",
    "            return result\n",
    "            \n",
    "        except Exception as e:\n",
    "            print(f\"❌ 오류 발생: {e}\")\n",
    "            self.execution_log.append(f\"오류: {str(e)}\")\n",
    "            return input_json\n",
    "    \n",
    "    def _generate_final_summary(self, state: TechAnalysisState) -> TechAnalysisState:\n",
    "        print(\"📝 [5단계] 최종 요약 생성\")\n",
    "        \n",
    "        # 간소화된 요약 생성\n",
    "        company_name = state.get(\"company_name\", \"\")\n",
    "        core_tech = state.get(\"core_tech\", \"\")\n",
    "        \n",
    "        state.update({\n",
    "            \"tech_summary\": f\"{company_name}의 {core_tech} 기술은 웹 검색 및 다각도 분석을 통해 검증된 혁신적 기술입니다. 시장에서 차별화된 가치를 제공할 수 있는 잠재력을 보유하고 있습니다.\",\n",
    "            \"strengths_and_weaknesses\": f\"강점: {core_tech}의 기술적 우수성과 특허 경쟁력. 약점: 시장 진입 장벽과 기술 구현의 복잡성\",\n",
    "            \"differentiation_points\": f\"{core_tech} 분야에서의 독특한 기술적 접근법과 웹 검색을 통해 확인된 시장 차별화 요소\",\n",
    "            \"technical_risks\": \"기술 구현의 난이도, 확장성 문제, 시장 수용성 및 규제 리스크 존재\",\n",
    "            \"patents_and_papers\": [\n",
    "                {\"type\": \"patent\", \"description\": f\"{core_tech} 관련 핵심 특허 포트폴리오\"},\n",
    "                {\"type\": \"research\", \"description\": \"웹 검색을 통해 확인된 기술 연구 성과\"}\n",
    "            ],\n",
    "            \"analysis_complete\": True\n",
    "        })\n",
    "        \n",
    "        print(\"  ✅ 최종 요약 완료\")\n",
    "        return state\n",
    "    \n",
    "    def _generate_basic_summary(self, state: TechAnalysisState) -> TechAnalysisState:\n",
    "        print(\"📝 기본 요약 생성\")\n",
    "        \n",
    "        state.update({\n",
    "            \"tech_summary\": f\"{state.get('company_name', '')}의 {state.get('core_tech', '')} - 추가 검증 필요\",\n",
    "            \"strengths_and_weaknesses\": \"제한된 정보로 인한 기본 분석\",\n",
    "            \"differentiation_points\": \"차별점 분석을 위한 추가 정보 필요\",\n",
    "            \"technical_risks\": \"기술 검증 부족으로 인한 높은 불확실성\",\n",
    "            \"patents_and_papers\": [],\n",
    "            \"analysis_complete\": False\n",
    "        })\n",
    "        \n",
    "        return state\n",
    "    \n",
    "    def get_execution_log(self):\n",
    "        return self.execution_log\n",
    "\n",
    "print(\"✅ 메인 에이전트 정의 완료\")"
   ]
  },
